<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
    <link rel="stylesheet" href="/css/style.css">
    <title>Astronomical Catalog</title>
</head>
<body>
    <%- include('./partials/header.ejs') %> 
    
    <main>
        <section class="cards">
            <% tabelaAstro.forEach(element => { %>
             <a href="details/<%=element.id%>">
             <div class="card">
                <div id="card-image"><img src="<%=element.image%>" alt="<%=element.name%>" id="image"></div>
                <div id="card-texto">
                    <div id="card-name"><p><%=element.name%></p></div>
                    <div id="card-nick"><h3><%=element.nickname%></div>
                </div>
            </div>
            </a>
            <div class="btncontainer" >
            <a href="/delete/<%= element.id %>"> <i class="fa-solid fa-trash"></i> </a>
            <a href="/edit/<%= element.id %>"><i class="fa-solid fa-pencil"></i></a>
            </div>
            <% }) %>
        </section>
    </main>

    <%- include('./partials/footer.ejs') %> 
</body>
</html>